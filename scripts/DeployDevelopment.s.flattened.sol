            assertEq(
                expected.nft.ownerOf(token.id),
                expected.owner,
                "invalid NFT owner"
            );

            (address pool, int24 lowerTick, int24 upperTick) = expected
                .nft
                .positions(token.id);

            assertEq(pool, token.pool, "invalid NFT position pool");
            assertEq(
                lowerTick,
                token.lowerTick,
